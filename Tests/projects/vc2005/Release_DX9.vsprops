<?xml version="1.0" encoding="shift_jis"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="Release_DX9"
	OutputDirectory="$(ProjectName)\$(ConfigurationName)"
	IntermediateDirectory="$(ProjectName)\$(ConfigurationName)"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="..\..\..\Libs\include;..\..\system;..\..\..\Samples\SampleKit"
		WarningLevel="3"
		Detect64BitPortabilityProblems="true"
		DebugInformationFormat="3"
		DisableSpecificWarnings="4819"
	/>
	<Tool
		Name="VCLinkerTool"
		AdditionalDependencies="d3d9.lib d3dx9.lib dxguid.lib dinput8.lib dxerr.lib winmm.lib Std.lib Graph.lib Io.lib Math.lib DebugUtil.lib System.lib SceneGraph.lib $(SolutionDir)..\..\..\Samples\SampleKit\$(ConfigurationName)\SampleKit.lib"
		OutputFile="$(ProjectDir)..\..\$(ProjectName)\$(ProjectName).exe"
		LinkIncremental="1"
		AdditionalLibraryDirectories="&quot;..\..\..\Libs\project\lib\$(ConfigurationName)&quot;"
		GenerateDebugInformation="true"
	/>
</VisualStudioPropertySheet>
